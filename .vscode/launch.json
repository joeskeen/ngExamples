{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Server",
			"type": "node",
			"request": "launch",
			"program": "mars/server/server.js",
			"stopOnEntry": false,
			"args": [],
			"cwd": "./mars",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"sourceMaps": false,
			"outDir": null
		},
		{
			"name": "Attach to Node",
			"type": "node",
			"request": "attach",
			"port": 5858
		}
	]
}